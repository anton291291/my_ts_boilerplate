        "webpack-node-externals": "^1.7.2"
    },
    "jest": {
        "collectCoverage": true,
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js",
            "jest-localstorage-mock"
        ],
        "transformIgnorePatterns": [
            "/node_modules/(?!@material-ui)",
            "/node_modules/(?!@babel)"
        ],
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "moduleNameMapper": {
            "@/(.*)": "<rootDir>/src/$1",
            "database": "<rootDir>/src/lib/database"
        }
    },
    "browserslist": [
        "> 1%",
        "last 3 version",
        "ie 11"
    ],
    "loki": {
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.app",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.app",
                "preset": "iPhone 7"
            }
        }
    }
